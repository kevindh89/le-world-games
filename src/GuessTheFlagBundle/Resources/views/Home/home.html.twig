{% extends "::base.html.twig" %}

{% block title %}GuessTheFlagBundle:Home:home{% endblock %}

{% block body %}
    <div>
        <a class="btn btn-default" href="{{ path('admin_overview') }}">Admin panel</a>
    </div>

    <h1>Welcome to the Flag game</h1>

    <ul>
        {% for flag in randomFlags %}
            <li>
                <img src="{{ flag.image }}" alt="Flag" />
            </li>
        {% endfor %}
    </ul>
{% endblock %}
